<div class="container" id="sign-up-form">
  <div class="row">
    <h1>Edit feed</h1>

    <div class="col-sm-offset-4 col-sm-4">
        <%= bootstrap_form_for(@feed) do |f| %>

        <%= f.text_field :title,
                         placeholder: 'Feed title',
                         class: 'form-control' %>
        <%= f.submit 'Update!', class: 'btn sign-up-button' %>
      <% end %>

    </div>
  </div>
  <%= link_to 'Show', @feed %> |
  <%= link_to 'Back', feeds_path %>
</div>

