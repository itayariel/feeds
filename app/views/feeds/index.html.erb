<p> Hello <%= current_user.name %></p>
<div class="container">
  <% @feeds.each do |feed| %>
    <%= link_to feed do%>
      <div class="card text-center text-white bg-success mb-3" style="width: 30rem;">

        <img class="card-img-top" src="<%= feed.img_link %>" alt="Card image cap" style="width: 28rem;">
        <div class="card-body">
          <h5 class="card-title"><%= feed.title %></h5>
        </div>
      </div>
    <% end %>
  <% end %>
  <br>
  <br>
  <%= link_to '+ New Feed', new_feed_path, class: 'btn btn-primary' %>
</div><!-- container -->

